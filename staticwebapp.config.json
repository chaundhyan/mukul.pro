{
  "routes": [
    {
      "route": "/*",
      "rewrite": "/html/utility.html"
    }
  ],
  "responseOverrides": {
    "400" : {
      "rewrite": "/html/utility.html"
    },
    "401": {
      "redirect": "/html/utility.html",
      "statusCode": 302
    },
    "403": {
      "rewrite": "/html/utility.html"
    },
    "404": {
      "rewrite": "/html/utility.html"
    }
  },
  "defaultHeaders": {
    "content-security-policy": "default-src https: 'unsafe-eval' 'unsafe-inline'; object-src 'none'"
  },
  "mimeTypes": {
    "custom": "text/html"
  }
}