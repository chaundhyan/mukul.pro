{
  "routes": [
    {
      "route": "/",
      "redirect": "/html/utility.html"
    }
  ],
  "responseOverrides": {
    "400" : {
      "redirect": "/html/utility.html"
    },
    "401": {
      "redirect": "/html/utility.html",
      "statusCode": 302
    },
    "403": {
      "redirect": "/html/utility.html"
    },
    "404": {
      "redirect": "/html/utility.html"
    }
  },
  "defaultHeaders": {
    "content-security-policy": "default-src https: 'unsafe-eval' 'unsafe-inline'; object-src 'none'"
  },
  "mimeTypes": {
    ".css": "text/css",
    ".js": "text/javascript",
    ".pdf": "application/pdf",
    ".html": "text/html"
  }
}